int moisturePin = A8; // Sensor output connected to A0
int moistureValue = 0; // Variable to store the sensor value

void setup() {
  Serial.begin(9600); // Start serial port at 9600 bps
}

void loop() {
  moistureValue = analogRead(moisturePin); // Read the analog value
  Serial.print("Moisture Level: ");
  Serial.println(moistureValue); // Print the value to the Serial Monitor
  delay(1000); // Wait for a second before the next read
}



#out in the air the moisture is around >580 (from my experiments) 
